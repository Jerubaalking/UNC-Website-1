<style>
    .ranks {
        margin: 0px;
        width: 100%;
        display: inline-flex;
    }

    .ranks header {
        font-size: 18px;
        font-weight: 600;

    }

    .ranks .info h2 {
        color: #00003aab;
        font-size: 20px;
        font-weight: 600;
        margin: 5px;

    }

    .ranks .info .champ-title {
        color: rgba(223, 134, 0, 0.912);
        text-decoration: underline;
        font-size: 14px;
        font-weight: 600;
        margin-top: 5px;
        margin-left: -12px;

    }

    .ranks .rank-cat .athletes .athlete-name h4 {
        font-weight: 600;
        color: #00003aab;
        font-size: 16px;
    }

    .ranks .rank-cat .athletes .rank .athlete-rank {
        font-weight: 600;
        font-size: 14px;
        margin-left: 15px;
        margin-right: 5px;
    }

    .rank-cat {
        margin: 1px;
        border: 0.1mm solid #61616133;
        background-color: rgba(232, 232, 232, 0.33);
    }

    .rank-cat:hover {
        box-shadow: 3px 2px 2px 2px rgba(0, 0, 0, 0.2);
    }

    .athlete-rankings-column {
        margin: 15px;
    }
</style>
<div class="view-items-wrp">
    <div data-drupal-views-infinite-scroll-content-wrapper="" class="views-infinite-scroll-content-wrapper clearfix">

        {{!-- all items/news blocks --}}
        <div class="trending-page-grid trending-page">
            {{#each rankings}}
            <div class="grid-item position_{{rankings}} size_1x1">

                <a data-aos="fade-up" class="c-card--grid-card-trending grid_card_image_text" href="/trending/all?#"
                    style="text-decoration: none;">

                    <div class="c-card--grid-card-trending__image">

                        <div
                            class="field field--name-thumbnail field--type-entity-reference field--label-hidden field__item">
                            <div class="layout layout--onecol">
                                <div class="layout__region layout__region--content">
                                    <img src="/public/images/kick.jpg"
                                        alt="this is alt text that describes the picture in case it cannot load for whatever reason."
                                        width="5088" height="3392" style="z-index:0;">
                                    <span class="c-card--grid-card-trending__info-prefix"
                                        style="color:aliceblue; z-index:0;margin-top:150;">Champion Name</span>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="c-card--grid-card-trending__3-wrapper">
                        <div class="c-card--grid-card-trending__info">
                            <span class="c-card--grid-card-trending__info-prefix">Champion Name</span>
                            <span class="c-card--grid-card-trending__info-suffix">
                                <div
                                    class="field field--name-datetime field--type-datetime field--label-hidden field__item">
                                    {{created}}</div>
                            </span>
                        </div>

                        <div class="c-card--grid-card-trending__2-wrapper">

                            <p class="c-card--grid-card e-t5" style="font-family: stolzl-bold;">
                                <span style="color: orangered;">Feather</span> weight
                            </p>


                            <div class="c-card--grid-card-trending__body">


                                <table class="athletes">
                                    <tbody>
                                        {{#each this}}
                                        <tr>
                                            <td class="rank">
                                                <span class="athlete-rank"
                                                    style="font-family: stolzl-bold; color:rgb(9, 9, 9);">{{val}}.
                                                </span>
                                            </td>
                                            <td>
                                                <a class="athlete-name" target="_blank"
                                                    href="/athletes/Bokang-Masunyane">
                                                    <h4 style="font-weight: 600; margin:2px; color:rgba(0, 0, 0, 0.7);">
                                                        Bokang Masunyane </h4>
                                                </a>
                                            </td>
                                            <td class="shift">
                                            </td>
                                        </tr>
                                        {{/each}}
                                    </tbody>
                                </table>

                            </div>



                        </div>


                    </div>


                </a>


            </div>
            {{/each}}
        </div>
    </div>
</div>